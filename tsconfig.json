{
    "compilerOptions": {
        "target": "ESNext",
        "types": ["node", "webpack-env", "@emotion/react/types/css-prop"],
        "jsx": "react",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", ".storybook/**/*.ts", ".storybook/**/*.tsx"]
}
