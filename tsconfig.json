{
    "compilerOptions": {
        "target": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "strict": true,
        "jsxImportSource": "@emotion/react",
        "skipLibCheck": true
    },
    "include": ["src"]
}
