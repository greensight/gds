{
    "compilerOptions": {
        "target": "ESNext",
        "types": ["node", "webpack-env", "@emotion/react/types/css-prop", "vite/client"],
        "jsx": "react",
        "module": "es2020",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", ".storybook/**/*.ts", ".storybook/**/*.tsx"]
}
