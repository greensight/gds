{
    "name": "@greensight/gds",
    "version": "0.12.0",
    "repository": "https://github.com/greensight/gds",
    "description": "Greensight Design System",
    "license": "MIT",
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "files": [
        "cjs/*",
        "esm/*",
        "bin/*"
    ],
    "sideEffects": [
        "*.css"
    ],
    "bin": {
        "gds-tokens": "bin/index.js"
    },
    "keywords": [
        "GDS",
        "Greensight",
        "Design System"
    ],
    "engines": {
        "node": ">= 11.7.0",
        "yarn": ">= 1.19.0"
    },
    "scripts": {
        "storybook": "start-storybook -s ./public -p 3000 --ci",
        "storybook:build": "build-storybook -o docs -s ./public",
        "storybook:serve": "serve docs -l 8080",
        "tokens": "node bin/index.js",
        "tokens:dev": "node bin/index.js -c gds-dev.config.json",
        "deploy": "yarn storybook:build && storybook-to-ghpages -e docs",
        "build": "rimraf '{cjs,esm}' && rollup -c",
        "eslint": "eslint . --ext js,ts,tsx --fix",
        "prepublishOnly": "yarn build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "cjs",
        "esm",
        "docs",
        "!.*"
    ],
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@emotion/babel-preset-css-prop": "^10.0.27",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@storybook/addon-a11y": "^5.3.12",
        "@storybook/addon-actions": "^5.3.12",
        "@storybook/addon-backgrounds": "^5.3.12",
        "@storybook/addon-docs": "^5.3.12",
        "@storybook/addon-knobs": "^5.3.12",
        "@storybook/addon-viewport": "^5.3.12",
        "@storybook/addons": "^5.3.12",
        "@storybook/react": "^5.3.12",
        "@storybook/storybook-deployer": "^2.8.1",
        "@storybook/theming": "^5.3.12",
        "@svgr/rollup": "^5.1.0",
        "@svgr/webpack": "^5.1.0",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "axios": "^0.19.2",
        "babel-loader": "^8.0.6",
        "chalk": "^3.0.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^2.3.0",
        "husky": "^4.2.1",
        "lint-staged": "^10.0.7",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-docgen-typescript-loader": "^3.6.0",
        "react-dom": "^16.12.0",
        "rimraf": "^3.0.2",
        "rollup": "^1.31.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-postcss": "^2.0.6",
        "serve": "^11.3.0",
        "storybook-addon-designs": "^5.2.0",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5"
    },
    "dependencies": {
        "@emotion/core": "^10.0.27",
        "@tippy.js/react": "^3.1.1",
        "emotion-theming": "^10.0.27",
        "focus-visible": "^5.0.2",
        "normalize.css": "^8.0.1",
        "tippy.js": "^5.2.0"
    }
}
