{
    "name": "@greensight/gds",
    "version": "3.1.3",
    "description": "Greensight Design System",
    "keywords": [
        "GDS",
        "Greensight",
        "Design System"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/greensight/gds.git"
    },
    "homepage": "https://github.com/greensight/gds",
    "license": "MIT",
    "sideEffects": false,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "types/src",
    "files": [
        "esm/*",
        "cjs/*",
        "types/*",
        "bin/*"
    ],
    "bin": {
        "gds-scss-tokens": "bin/scss/index.js",
        "gds-tokens": "bin/index.js"
    },
    "scripts": {
        "build": "run-s clear build:main",
        "build:main": "run-p build:types build:rollup",
        "build:rollup": "rollup -c",
        "build:types": "yarn pnpify tsc -p tsconfig.types.json",
        "clear": "rimraf '{esm,cjs,types}'",
        "deploy": "yarn storybook:build && storybook-to-ghpages -e docs",
        "eslint": "eslint . --ext .js,.ts,.tsx,.mdx --fix",
        "lint-staged": "lint-staged",
        "prepublishOnly": "yarn build",
        "storybook": "start-storybook -s ./public -p 3000 --ci",
        "storybook:build": "build-storybook -o docs -s ./public",
        "storybook:serve": "serve docs -l 8080",
        "tokens": "node bin/index.js",
        "tokens:dev": "node bin/index.js -c gds-dev.config.json",
        "tsc": "yarn pnpify tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "run-p lint-staged tsc"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,mdx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "public",
        "esm",
        "cjs",
        "types",
        "docs",
        "!/.storybook"
    ],
    "dependencies": {
        "@emotion/react": "11.11.4",
        "@emotion/styled": "11.11.5",
        "@tippyjs/react": "4.2.6",
        "chalk": "4.1.1",
        "deepmerge": "^4.2.2",
        "focus-visible": "^5.0.2",
        "node-fetch-commonjs": "^3.3.2",
        "normalize.css": "8.0.1",
        "tippy.js": "6.3.7",
        "ts-dedent": "^2.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.5",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/runtime": "^7.12.5",
        "@babel/types": "^7.12.10",
        "@emotion/babel-plugin": "^11.10.2",
        "@emotion/babel-preset-css-prop": "11.2.0",
        "@emotion/css": "11.11.2",
        "@emotion/serialize": "1.1.2",
        "@emotion/utils": "1.2.0",
        "@mdx-js/loader": "^1.5.8",
        "@mdx-js/react": "^1.5.8",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@storybook/addon-a11y": "^6.5.14",
        "@storybook/addon-actions": "6.5.14",
        "@storybook/addon-backgrounds": "^6.5.14",
        "@storybook/addon-controls": "^6.5.14",
        "@storybook/addon-docs": "6.5.14",
        "@storybook/addon-viewport": "^6.5.14",
        "@storybook/builder-webpack5": "6.5.14",
        "@storybook/client-api": "^6.5.14",
        "@storybook/manager-webpack5": "^6.5.14",
        "@storybook/react": "6.5.14",
        "@storybook/storybook-deployer": "^2.8.16",
        "@svgr/rollup": "^5.4.0",
        "@svgr/webpack": "latest",
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/babel__core": "7.1.10",
        "@types/chalk": "2.2.0",
        "@types/core-js": "2.5.4",
        "@types/eslint": "7.2.4",
        "@types/eslint-plugin-prettier": "3.1.0",
        "@types/mdx-js__react": "1.5.3",
        "@types/node": "^18.11.17",
        "@types/prettier": "2.1.1",
        "@types/react": "18.2.0",
        "@types/react-is": "17.0.0",
        "@types/rimraf": "3.0.0",
        "@types/rollup-plugin-commonjs": "9.3.1",
        "@types/svgr__rollup": "4.3.0",
        "@types/webpack": "5.28.0",
        "@types/webpack-env": "^1.18.0",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "@yarnpkg/pnpify": "^2.4.0",
        "babel-loader": "^8.1.0",
        "core-js": "^3.6.5",
        "eslint": "^8",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-mdx": "^2.0.5",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.6.8",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "react": "18.3.1",
        "react-docgen-typescript-plugin": "^1.0.2",
        "react-dom": "18.3.1",
        "react-is": "17.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.1",
        "rollup-plugin-postcss": "^4.0.2",
        "serve": "^14.1.2",
        "storybook-addon-designs": "^6.3.1",
        "ts-node": "^9.0.0",
        "typescript": "4.6.4",
        "vite": "^4.5.3",
        "webpack": "^5.91.0",
        "worker-loader": "^2.0.0"
    },
    "peerDependencies": {
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "engines": {
        "node": ">= 16.0.0"
    },
    "packageManager": "yarn@3.3.1",
    "bugs": {
        "url": "https://github.com/greensight/gds/issues"
    }
}
