{
    "name": "@greensight/gds",
    "version": "2.1.0",
    "description": "Greensight Design System",
    "keywords": [
        "GDS",
        "Greensight",
        "Design System"
    ],
    "repository": "https://github.com/greensight/gds",
    "homepage": "https://github.com/greensight/gds",
    "license": "MIT",
    "sideEffects": false,
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "types/src",
    "files": [
        "esm/*",
        "cjs/*",
        "types/*",
        "bin/*"
    ],
    "bin": {
        "gds-tokens": "bin/index.js"
    },
    "scripts": {
        "build": "run-s clear build:main",
        "build:main": "run-p build:types build:rollup",
        "build:rollup": "rollup -c",
        "build:types": "yarn pnpify tsc -p tsconfig.types.json",
        "clear": "rimraf '{esm,cjs,types}'",
        "deploy": "yarn storybook:build && storybook-to-ghpages -e docs",
        "eslint": "eslint . --ext .js,.ts,.tsx,.mdx --fix",
        "lint-staged": "lint-staged",
        "prepublishOnly": "yarn build",
        "storybook": "start-storybook -s ./public -p 3000 --ci",
        "storybook:build": "build-storybook -o docs -s ./public",
        "storybook:serve": "serve docs -l 8080",
        "tokens": "node bin/index.js",
        "tokens:dev": "node bin/index.js -c gds-dev.config.json",
        "tsc": "yarn pnpify tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "run-p lint-staged tsc"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,mdx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "public",
        "esm",
        "cjs",
        "types",
        "docs",
        "!/.storybook"
    ],
    "dependencies": {
        "@emotion/core": "^10.0.28",
        "@tippy.js/react": "^3.1.1",
        "@types/react": "^16.9.31",
        "deepmerge": "^4.2.2",
        "emotion-theming": "^10.0.27",
        "focus-visible": "^5.0.2",
        "normalize.css": "^8.0.1",
        "tippy.js": "^5.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@babel/types": "^7.9.0",
        "@emotion/babel-preset-css-prop": "^10.0.27",
        "@emotion/css": "^10.0.27",
        "@emotion/serialize": "^0.11.16",
        "@emotion/utils": "^0.11.3",
        "@mdx-js/loader": "^1.5.8",
        "@mdx-js/react": "^1.5.8",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@storybook/addon-a11y": "6.0.0-alpha.20",
        "@storybook/addon-actions": "6.0.0-alpha.20",
        "@storybook/addon-backgrounds": "6.0.0-alpha.20",
        "@storybook/addon-docs": "6.0.0-alpha.20",
        "@storybook/addon-knobs": "6.0.0-alpha.20",
        "@storybook/addon-viewport": "6.0.0-alpha.20",
        "@storybook/client-api": "6.0.0-alpha.20",
        "@storybook/react": "6.0.0-alpha.20",
        "@storybook/storybook-deployer": "^2.8.3",
        "@svgr/rollup": "^5.3.0",
        "@svgr/webpack": "^5.3.0",
        "@types/babel__core": "7.1.7",
        "@types/chalk": "2.2.0",
        "@types/core-js": "2.5.3",
        "@types/eslint": "6.1.9",
        "@types/eslint-plugin-prettier": "2.2.0",
        "@types/mdx-js__react": "1.5.1",
        "@types/prettier": "1.19.1",
        "@types/react-dom": "16.9.6",
        "@types/react-is": "16.7.1",
        "@types/rimraf": "3.0.0",
        "@types/rollup-plugin-commonjs": "9.3.1",
        "@types/svgr__rollup": "4.3.0",
        "@types/webpack": "4.41.10",
        "@types/webpack-env": "1.15.1",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "@yarnpkg/pnpify": "^2.0.0-rc.20",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "chalk": "^3.0.0",
        "core-js": "^3.6.4",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-import-resolver-node": "^0.3.3",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-mdx": "^1.6.8",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "husky": "^4.2.3",
        "lint-staged": "^10.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.2",
        "react": "^16.13.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "react-is": "^16.13.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.3.2",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-postcss": "^2.5.0",
        "serve": "^11.3.0",
        "storybook-addon-designs": "^5.2.0",
        "ts-node": "^8.8.1",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "worker-loader": "^2.0.0"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "engines": {
        "node": ">= 11.7.0"
    }
}
