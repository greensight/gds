{
    "name": "@greensight/gds",
    "version": "0.10.2",
    "repository": "https://github.com/greensight/gds",
    "description": "Greensight Design System",
    "license": "MIT",
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "files": [
        "cjs/*",
        "esm/*",
        "bin/*"
    ],
    "sideEffects": [
        "*.css"
    ],
    "bin": {
        "gds-tokens": "bin/index.js"
    },
    "keywords": [
        "GDS",
        "Greensight",
        "Design System"
    ],
    "engines": {
        "node": ">= 12.13.0",
        "yarn": ">= 1.19.0"
    },
    "browserslist": [
        "chrome >= 67",
        "ios >= 11",
        "safari >= 12",
        "firefox >= 67",
        "edge >= 17",
        "opera >= 60"
    ],
    "scripts": {
        "build": "rimraf '{cjs,esm}' && rollup -c",
        "storybook": "start-storybook -s ./public --ci",
        "storybook:build": "build-storybook -o docs -s ./public",
        "storybook:serve": "serve docs -l 5000",
        "storybook:deploy": "storybook-to-ghpages -o docs",
        "tokens": "node bin/index.js",
        "eslint": "eslint . --ext js,jsx,ts,tsx --fix",
        "stylelint": "stylelint '**/*.css' --fix",
        "prepublishOnly": "yarn build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.css": [
            "stylelint --fix",
            "git add"
        ]
    },
    "eslintIgnore": [
        "node_modules",
        "cjs",
        "esm",
        "docs",
        "!.*"
    ],
    "peerDependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/preset-typescript": "^7.7.2",
        "@babel/register": "^7.7.0",
        "@emotion/babel-preset-css-prop": "^10.0.23",
        "@rollup/plugin-json": "^4.0.0",
        "@rollup/plugin-node-resolve": "^6.0.0",
        "@storybook/addon-a11y": "^5.2.8",
        "@storybook/addon-actions": "^5.2.8",
        "@storybook/addon-backgrounds": "^5.2.8",
        "@storybook/addon-docs": "^5.2.8",
        "@storybook/addon-knobs": "^5.2.8",
        "@storybook/addon-viewport": "^5.2.8",
        "@storybook/addons": "^5.2.8",
        "@storybook/react": "^5.2.8",
        "@storybook/storybook-deployer": "^2.8.1",
        "@storybook/theming": "^5.2.8",
        "@svgr/rollup": "^4.3.3",
        "@svgr/webpack": "^4.3.3",
        "@typescript-eslint/eslint-plugin": "^2.8.0",
        "@typescript-eslint/parser": "^2.8.0",
        "axios": "^0.19.0",
        "babel-loader": "^8.0.6",
        "browserslist": "^4.7.3",
        "chalk": "^3.0.0",
        "css-loader": "^3.2.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-mdx": "^1.6.5",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "extract-css-chunks-webpack-plugin": "^4.6.0",
        "focus-visible": "^5.0.2",
        "husky": "^3.1.0",
        "lint-staged": "^9.5.0",
        "postcss-inline-svg": "^4.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-mixins": "^6.2.3",
        "postcss-nested": "^4.2.1",
        "postcss-normalize": "^8.0.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-pxtorem": "^4.0.1",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-docgen-typescript-loader": "^3.5.0",
        "react-dom": "^16.12.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.27.3",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-postcss": "^2.0.3",
        "serve": "^11.2.0",
        "stylelint": "^12.0.0",
        "stylelint-config-prettier": "^6.0.0",
        "stylelint-config-standard": "^19.0.0",
        "stylelint-prettier": "^1.1.1",
        "svg-url-loader": "^3.0.2",
        "typescript": "^3.7.2",
        "url-loader": "^2.2.0"
    },
    "dependencies": {
        "@emotion/core": "^10.0.22",
        "@emotion/styled": "^10.0.23",
        "@tippy.js/react": "^3.1.1",
        "core-js": "^3.4.1",
        "emotion-theming": "^10.0.19",
        "styled-system": "^5.1.2",
        "tippy.js": "^5.1.2"
    }
}
